    },
    {
      "type": "packages",
      "format": "{} (pacman)",
      "key": " "
    },
    {
      "type": "shell",
      "key": " "
    },
    {
      "type": "terminal",
      "key": " "
    },
    {
      "type": "wm",
      "key": " "
    },
    {
      "type": "uptime",
      "key": " "
    },
    "break"
  ]
}
