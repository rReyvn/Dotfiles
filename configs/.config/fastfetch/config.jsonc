{
  "$schema": "https://github.com/fastfetch-cli/fastfetch/raw/dev/doc/json_schema.json",
  "logo": {
    "source": "~/.config/fastfetch/graveyard.png",
    "padding": {
      "top": 0,
      "left": 1
    }
  },
  "display": {
    "separator": " "
  },
  "modules": [
    "break",
    "break",
    {
